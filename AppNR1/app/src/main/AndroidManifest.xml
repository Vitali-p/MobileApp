<?xml version="1.0" encoding="utf-8"?>
<manifest
    xmlns:android="http://schemas.android.com/apk/res/android"
    package="vitali.appnr1">  <!-- (unikke navn eller appens unike ID) for app'ens pakkenavn  (det er en mappe på telefoents filsystem som få dette navn) -->

    <!-- Permissions -->
    <uses-permission android:name="android.permission.INTERNET"/>  <!-- Permission to get access to internet -->

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- synlig aktivitet på startskærmen -->
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

    </application>

</manifest>